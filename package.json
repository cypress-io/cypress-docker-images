{
  "name": "cypress-docker-images",
  "version": "1.0.0",
  "description": "These images provide all of the required dependencies for running Cypress in Docker.",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "check:markdown": "find . -type f -name '*.md' ! -path '*/node_modules/*' | xargs -L1 npx markdown-link-check -c markdownLinkConfig.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint"
  },
  "engines": {
    "node": ">=18.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypress-io/cypress-docker-images.git"
  },
  "keywords": [],
  "author": "Cypress-io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cypress-io/cypress-docker-images/issues"
  },
  "homepage": "https://github.com/cypress-io/cypress-docker-images#readme",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^4.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-cypress": "^4.2.0",
    "globals": "^16.0.0",
    "markdown-link-check": "3.12.2",
    "prettier": "3.5.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
